<div class="col-md-10 center-block upcoming-programs-form padding-bottom-top">
  <h1 style="text-align: center;">Edit Contact Form</h1>
  <%= form_for @content do |f| %>
    <% if @content.errors.any?%>
      <h2><%= pluralize(@content.errors.count, "error") %> prevented this client from saving:</h2>
      <ul>
        <% @content.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <%end%>
      </ul>
    <%end%>

    <div class="form-group">
      <%= f.label :contact_bg %>
      <% if @content.contact_bg.exists? %>
        <%= image_tag @content.contact_bg.url(:medium) %>
      <%end%>
      <%= f.file_field :contact_bg, class: "validate"%>
    </div>

    <div class="form-group">
      <%= f.label :contact_title %>
      <%= f.text_field :contact_title, {class: 'form-control form-input'} %>
    </div>

    <div class="form-group">
      <%= f.label :contact_subtitle %>
      <%= f.text_field :contact_subtitle, {class: 'form-control form-input'} %>
    </div>



    <button type="submit" class="btn sendbtn">Submit</button>

  <%end%>
</div>
